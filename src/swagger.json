{
  "openapi": "3.0.0",
  "info": {
    "title": "rentx",
    "description": "app for rental car",
    "version": "1.0.0",
    "contact": {
      "email": "erivaldomontez@gmail.com"
    }
  },
  "paths": {
    "/categories": {
      "post": {
        "tags": [
          "category"
        ],
        "summary": "create",
        "description": "create a new category car",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "propreties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "category name sample",
                  "description": "category description sample"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "category created"
          },
          "500": {
            "description": "category already exists"
          }
        }
      },
      "get": {
        "tags": [
          "category"
        ],
        "summary": "list",
        "description": "list all categories",
        "responses": {
          "200": {
            "descritption": "list categories",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/categories/import": {
      "post": {
        "tags": [
          "category"
        ],
        "summary": "import category",
        "description": "upload a new category",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "created"
          }
        }
      }
    },
    "/specifications": {
      "post": {
        "tags": [
          "specifications"
        ],
        "summary": "create",
        "description": "create specification for a car",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/Specification"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "created"
          },
          "500": {
            "description": "specification already exists"
          }
        }
      }
    }
  },
  "definitions": {
    "Specification": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      }
    }
  }
}